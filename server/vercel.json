{
    "version" : 2,
    "builds" : [{
        "src" : "src/index.js", 
        "use" : "@vercel/node" 
    }],
    "routes" : [{
        "src" : "/(.*)",
        "dest" : "src/index.js",
        "methods" : ["GET" , "POST", "PUSH", "DELETE", "PATCH", "OPTIONS"],
        "headers" : [
            { "key": "Access-Control-Allow-Origin", "value": "*" },
            {  "key": "Access-Control-Allow-Credentials", "value": "true" }
        ]
    }]
}